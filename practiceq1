#include<stdio.h>
#include<stdlib.h>


int main (int argc, int *argv[]) {
	void *p[2], *q, *r;
	p[0] = q;
	p[1] = r;

	int i,j;
	i=**argv;
	j=**argv+1);
	int *arr = (int *)malloc(i*sizeof(int));
	float *ptr = (float *)malloc(j*sizeof(float));
	
	printf("Enter %d integer numbers:", i);
	for (int k=0; k<i; k++) {
		scanf("%d", arr);
	}
	printf("Enter %d decimal numbers:", j);
	for (int k=0; k<j; k++) {
		scanf("%f", ptr);
	}
	q = (int *)arr;
	r = (float *)ptr;

	printf("The members of 2D array include:\n");
	for (int k=0; k<i; k++) {
		printf("%d, ", arr[k]);
	}
	printf("\n");
	for (int k=0; k<j; k++) {
		printf("%f, ", ptr[k]);
	}


	return 0;
}
